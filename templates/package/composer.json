{
    "name": "<%= vendorName %>/<%= packageName %>",
    "description": "<%= packageDescription %>",
    "license": "MIT",
    "authors": [
        {
            "name": "<%= authorName %>",
            "email": "<%= authorEmail %>"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "5.*",
        "markusjwetzel/laravel-handlebars": "~1.0@dev"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/commands",
            "src/models"
        ],
        "psr-0": {
            "<%= vendorNameCaps %>\\<%= packageNameCaps %>\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
